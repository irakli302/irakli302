#include <iostream>
using namespace std;

int main(){
    string s;
    int n,count1=0,count2=0;
    cin>>n>>s;
    for(int i=0;i<n;++i){
        if(s[i]=='A')++count1;
            else if(s[i]=='D')++count2;
    }
    if(count1>count2)cout<<"Anton";
        else if(count1<count2)cout<<"Danik";
            else cout<<"Friendship";
}
