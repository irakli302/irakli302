#include <iostream>
using namespace std;

int main() {
    string s = "codeforces";
    int t;
    cin >> t;
    while (t--)
    {
        int check = 0;
        char c;
        cin >> c;
        for (int i = 0; i < s.size(); ++i){
            if (s[i] == c) {
                check = 1;
                break;
            }
        }   
        if (check) cout << "YES" << endl;
            else cout << "NO" << endl; 
    }
    
}
