#include <iostream>
#include <set>
using namespace std;

int main(){
    int n,q,p,k;
    cin>>n>>p;
    set<int>st;
    while(p--){
        cin>>k;
        st.insert(k);
    }
    cin>>q;
    while(q--){
        cin>>k;
        st.insert(k);
    }
    if(st.size()==n)cout<<"I become the guy.";
        else cout<<"Oh, my keyboard!";
}
