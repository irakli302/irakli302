#include <iostream>
using namespace std;

int main(){
    int n,ans=0;
    cin>>n;
    string s;
    while(n--){
        cin>>s;
        if(s=="Icosahedron")ans+=20;
        if(s=="Cube")ans+=6;
        if(s=="Tetrahedron")ans+=4;
        if(s=="Dodecahedron")ans+=12;
        if(s=="Octahedron")ans+=8;
    }
    cout<<ans;
}
